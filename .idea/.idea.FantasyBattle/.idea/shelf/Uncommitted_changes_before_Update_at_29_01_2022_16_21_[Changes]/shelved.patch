+void WriteRandomFile(TArray<uint8>& Buffer, int32 Size)
+{
+	Buffer.Empty(Size);
+	Buffer.AddUninitialized(Size);
+	for (int32 i = 0; i < Size; i++)
+	{
+		Buffer[i] = i % 255;
+	}
+}
