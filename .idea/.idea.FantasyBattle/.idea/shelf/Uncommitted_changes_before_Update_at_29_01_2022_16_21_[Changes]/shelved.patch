+	FOnlineSubsystemModule& OSS = FModuleManager::GetModuleChecked<FOnlineSubsystemModule>("OnlineSubsystem");
+	OSS.UnregisterPlatformService(EOS_SUBSYSTEM);
+
+	delete EOSFactory;
+	EOSFactory = nullptr;
+}
